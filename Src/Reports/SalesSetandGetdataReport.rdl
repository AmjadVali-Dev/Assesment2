  <Code>'===========================================
' BLANK FUNCTIONS (KEEP THEM)
'===========================================
Public Function BlankZero(ByVal Value As Decimal)
    If Value = 0 Then
        Return ""
    End If
    Return Value
End Function

Public Function BlankPos(ByVal Value As Decimal)
    If Value &gt; 0 Then
        Return ""
    End If
    Return Value
End Function

Public Function BlankZeroAndPos(ByVal Value As Decimal)
    If Value &gt;= 0 Then
        Return ""
    End If
    Return Value
End Function

Public Function BlankNeg(ByVal Value As Decimal)
    If Value &lt; 0 Then
        Return ""
    End If
    Return Value
End Function

Public Function BlankNegAndZero(ByVal Value As Decimal)
    If Value &lt;= 0 Then
        Return ""
    End If
    Return Value
End Function


'===========================================
' FINAL SETDATA / GETDATA FOR SALES HEADER
' KEEP ONLY THIS VERSION
'===========================================
Shared HeaderData As String

Public Function SetData(NewData As Object) As Boolean
    If NewData Is Nothing OrElse CStr(NewData) = "" Then
        Return False
    End If

    HeaderData = CStr(NewData)
    Return True
End Function

Public Function GetData(Num As Integer) As Object
    If HeaderData Is Nothing OrElse HeaderData = "" Then
        Return ""
    End If

    Dim arr() As String = Split(HeaderData, Chr(177))

    If Num - 1 &gt;= 0 AndAlso Num - 1 &lt; arr.Length Then
        Return arr(Num - 1)
    End If

    Return ""
End Function</Code>
